{
  "manifestVersion": 1,
  "id": "team-retrospectives",
  "publisher": "ms-devlabs",
  "version": "1.70.6",
  "name": "Retrospectives",
  "description": "An Azure DevOps extension for efficient retrospectives.",
  "public": true,
  "icons": {
    "small": "images/logos/logo_small.png",
    "branding": "images/logos/logo_branding.png",
    "default": "images/logos/logo_default.png"
  },
  "branding": {
    "color": "#0065a3",
    "theme": "dark"
  },
  "content": {
    "details": {
      "path": "./assets/PACKAGE-DESCRIPTION.md"
    },
    "license": {
        "path": "./assets/LICENSE.md"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/4.1"
  ],
  "tags": [
    "agile",
    "feedback",
    "retrospective"
  ],
  "categories": [
    "Azure Boards"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/microsoft/vsts-extension-retrospectives"
  },
  "CustomerQnASupport": {
    "enableqna":"true"
  }, 
  "scopes": [
    "vso.work",
    "vso.work_write"
  ],
  "contributions": [
    {
      "id": "home",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Retrospectives",
        "uri": "src/index.html",
        "icon": {
            "light": "asset://images/logos/logo_navbar_light_theme.png",
            "dark": "asset://images/logos/logo_navbar_dark_theme.png"
        }
      }
    }
  ],
  "files": [
    {
      "path": "src",
      "addressable": true
    },
    {
      "path": "dist/reflect-bundle.js",
      "addressable": true
    },
    {
      "path": "node_modules/applicationinsights-js/dist",
      "addressable": true
    },
    {
      "path": "images/logos",
      "addressable": true
    },
    {
      "path": "images/screenshots",
      "addressable": true
    }
  ]
}